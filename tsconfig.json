{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "strict": true,
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": ["src/**/*"],
  "exclude": ["src/analytics/**/*"]
}